{
  "name": "enjoy-time",
  "description": "Enjoy your time with different time bar",
  "version": "0.2.3",
  "manifest_version": 2,
  "icons": {
    "16": "./icons/icon_16.png",
    "48": "./icons/icon_48.png",
    "128": "./icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "enjoy-time"
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "permissions": [
    "storage"
  ]
}
